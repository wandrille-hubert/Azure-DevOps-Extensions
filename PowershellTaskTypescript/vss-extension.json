{
    "manifestVersion": 1,
    "id": "PowershellTaskTypescript",
    "version": "0.0.3",
    "name": "Powershell Task Typescript",
    "description": "A Build Task to run a powershell script with inputs from typescript.",
    "publisher": "wany",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/logo.png"
    },
	"categories": [
        "Azure Pipelines"
    ],
    "contributions": [
        {
            "id": "PowershellTaskTypescript",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildtask"
            }
        }
    ],
    "files": [
        {
            "path": "buildtask",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
	"public": false
}